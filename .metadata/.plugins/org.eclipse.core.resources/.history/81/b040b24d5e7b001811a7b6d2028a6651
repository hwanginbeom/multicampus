/*작품 내용
 *  1. 오늘 배운 내용에 한 해서 3개의 메소드를 개발
 *  2. 반복 , 조건등에 대한 연습이 될 수 있는 하나의 lab 개발 
 *  3. 마음 : 내 짝꿍을 생각 하는 마음 
 */
package step01.syntax;

public class Step10MyArt {
	
	public static void forTest() { //   마름모 만들기
		int a=4;
		for(int i = 1 ; i <= 5; i++) {
			for(int j = 4 ; i<=j;  j--) {
				System.out.print(" ");
			}
			for(int z=10 ; z>a*2+1; z--) {
				System.out.print("*");
			}
			System.out.println();
			a--;

		}
		a=1;
		for(int i = 1 ; i <= 5; i++) {
			for(int j = 1; j<=i;  j++) {
				System.out.print(" ");
			}
			for(int z=10 ; z>a*2+1; z--) {
				System.out.print("*");
			}
			System.out.println();
			a++;

		}
		
	}
	
	
	public static String studytime(int time ,int time2) {
		
		String Salary = "5000";
		if (time >=400) {
			Salary = "Salary 5000";
		}
		else if (time>=380) {
			Salary = "Salary 4500";
		}
		else if (time>=360) {
			Salary = "Salary 4000";
		}
		else if (time>=340) {
			Salary = "Salary 3500";
		}
		else if(time>=320) {
			Salary = "Salary 3000";
		}
		else {
			Salary = "s"
					+ "ad stroy";
		}
		return Salary ;
		
	}

	public static void main(String[] args) {
		forTest();
		System.out.println(studytime(367,200));

	}

}
