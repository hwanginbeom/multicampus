package test5;

import step01.test.Test2;

public class tete{
	
	public String name = "황인범";
	public String city = "인천";
	public int age = 27;
	
	public tete(){}
	
	public tete(String n , String c , int a) {
		name = "꺄꺄";
		city = "끼욝?";
		age = 21;
	}
	
	public int output() {
		return 5+3;
	}
	
	public int output(int a , int b) {
		return a+b;
	}
	
	public void output1() {
		
	  System.out.println(100+200); 
	} // tete자체가 값이 있기 떄문에 void가 쓸 수없다.
	
	
	
	public static void main(String[] args) {
		
		tete t1 = new tete();
		t1.name = "끼끼욝";
		tete t2 = new tete( "a",  "b",25 );
		System.out.println(t1.name);
		System.out.println(t2.name);
		
		System.out.println(t1.output()); // 생성자 이름.메소드명
		
		
	}
}
